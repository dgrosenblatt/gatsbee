<div class="clubs-container">
  <div class="row">
    <div class="small-12 columns">
      <h1>All Clubs</h1>

      <% @clubs.each do |club| %>
        <h3><%= link_to club.name, club %></h3>
        <p><%= club.description %></p>
        <p>Currently Reading:</p>
      <% end %>
    </div>
  </div>
</div>
